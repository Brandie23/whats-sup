{
  "name": "sup",
  "version": "1.0.0",
  "description": "Sup",
  "main": "server/index.js",
  "scripts": {
    "postinstall": "webpack",
    "dev": "nodemon --watch server & webpack --watch",
    "dev-win-1": "nodemon --watch server",
    "dev-win-2": "webpack --watch",
    "start": "node server/index",
    "test": "jest",
    "watch": "nodemon -e js,jsx -x npm test",
    "h:deploy": "git push heroku master",
    "h:migrate": "heroku run knex migrate:latest",
    "h:rollback": "heroku run knex migrate:rollback",
    "h:seed": "heroku run knex seed:run",
    "knex": "knex",
    "update:packages": "node wipe-dependencies.js && rm -rf node_modules && npm update --save-dev && npm update --save"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "jest": {
    "setupFiles": [
      "./client/Test/setup-env.test.js"
    ]
  },
  "devDependencies": {
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "jest": "^23.6.0",
    "nodemon": "*",
    "supertest": "*"
  },
  "dependencies": {
    "@material-ui/core": "^3.6.1",
    "@material-ui/icons": "^3.0.1",
    "babel-core": "^6.26.3",
    "babel-loader": "7",
    "babel-preset-es2015": "*",
    "babel-preset-react": "*",
    "babel-preset-stage-0": "*",
    "babel-register": "*",
    "bcrypt": "*",
    "body-parser": "*",
    "cors": "*",
    "dotenv": "^6.2.0",
    "express": "*",
    "express-jwt": "*",
    "jsdom": "^13.0.0",
    "jsonwebtoken": "^8.4.0",
    "jsx-loader": "*",
    "jwt-decode": "*",
    "knex": "^0.15.2",
    "leaflet": "*",
    "node-pre-gyp": "^0.12.0",
    "passport": "*",
    "pg": "^7.7.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-leaflet": "*",
    "react-redux": "^6.0.0",
    "react-router-dom": "*",
    "react-router-redux": "^4.0.8",
    "react-test-renderer": "^16.6.3",
    "redux": "^4.0.1",
    "redux-thunk": "*",
    "sqlite3": "*",
    "superagent": "^4.0.0",
    "webpack": "^3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ptorrsmith/sup.git"
